---
import { getEntry, render } from 'astro:content';

import CommunityBanner from '@components/CommunityBanner.astro';
import ExplorerBanner from '@components/ExplorerBanner.astro';
import GitModifiedLayout from "@layouts/GitModifiedLayout.astro";

const dmca = await getEntry('policies', 'dmca');
if (!dmca) {
  throw new Error('Could not find dmca');
}
const { Content, remarkPluginFrontmatter } = await render(dmca);

---


<GitModifiedLayout pageTitle={remarkPluginFrontmatter.title}, frontmatter={ remarkPluginFrontmatter }>
    <Content />
    <CommunityBanner slot="banner" />
    <ExplorerBanner slot="banner-low" />
</GitModifiedLayout>
