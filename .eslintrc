{
  "extends": ["standard"],
  "env": {
    "es6": true,
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": [2, "always"],
    "no-extra-semi": 2,
    "semi-spacing": [2, { "before": false, "after": true }],
    "react/jsx-uses-vars": 2,
    "react/jsx-uses-react": 2,
    "react/prop-types": 2
  }
}
